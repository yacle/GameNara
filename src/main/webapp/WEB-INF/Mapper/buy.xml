<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="buy">
	<insert id="add" >
	insert into board values(buy_seq.nextval,#{title},#{detail},#{price},#{buy_id},#{end},#{categoly},sysdate)
	</insert>
	<select id="list">
	select * from BUY;
	</select>
	
</mapper>